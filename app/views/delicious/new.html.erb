<%= error_messages_for :delicious %>

<div id="delicious">
  <% form_for(:delicious, @delicious, :url => delicious_path, :html => { :method => :post, :id => 'deliciousform', :class => 'common-form' }) do |f| %>
  
  <table cellpadding="0" cellspacing="0">
  <tbody>

    <%= render :partial => 'form', :locals => { :f => f } %>
    
    <tr>
      <th>&nbsp;</th>
      <td>
        <%= f.submit t('common.create') + raw(' &rarr;'), :class => 'green awesome' %>
      </td>
    </tr>

  </tbody>
  </table>
    
  <% end %>
</div>